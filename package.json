{
  "name": "gatsby-transformer-kontent",
  "description": "Transforms assets from Kontent.ai into nodes usable by Gatsby Image",
  "version": "0.1.0",
  "author": "Richard Shackleton <richardshackleton@netconstruct.co.uk>",
  "bugs": {
    "url": "https://github.com/rshackleton/gatsby-transformer-kontent/issues"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.16.0",
    "@typescript-eslint/parser": "^2.16.0",
    "cross-env": "^6.0.3",
    "eslint": "^6.8.0",
    "prettier": "^1.19.1",
    "typescript": "^3.7.5"
  },
  "homepage": "https://github.com/rshackleton/gatsby-transformer-kontent/tree/master#readme",
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "kontent"
  ],
  "license": "MIT",
  "peerDependencies": {
    "gatsby": "^2.18.25"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rshackleton/gatsby-transformer-kontent.git"
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "eslint src/**/*",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepare": "cross-env NODE_ENV=production npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  }
}
