{
  "compilerOptions": {
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "module": "CommonJS",
    "moduleResolution": "Node",
    "outDir": "",
    "resolveJsonModule": true,
    "strict": true,
    "skipLibCheck": true,
    "target": "ES2015"
  },
  "include": ["src/**/*", "fragments.ts", "gatsby-node.ts"],
  "exclude": ["node_modules", "**/*.spec.ts"]
}
